<!--
    Document   : castExperiment.xml
    Created on : 06 September 2011, 15:00
    Author     : Jeremiah M. Via <jxv911@cs.bham.ac.uk>
    Description:

        This experiment definition will turn a classification .cl file into
        a CSV file of timestamp and scores.

            1. name
                    The name of the file you will load. Leave off the .cl
                    extension. The resulting CSV will have the same name but
                    with a .csv extension
            2. aucom:wd
                    The working directory where the file will be saved.
-->
<aucom:experiments xmlns:aucom="http://www.cor-lab.de">
    <aucom:experiment factory="org.bham.applications.experimenter.experiment.ClassificationToCSVFactory" name="debug">
        <aucom:input>
            <aucom:wd>/home/jxv911/Dropbox/aucom/cast</aucom:wd>
        </aucom:input>
    </aucom:experiment>
</aucom:experiments>