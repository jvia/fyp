<?xml version="1.0" encoding="UTF-8"?>
<aucom:experiments xmlns:aucom="http://www.cor-lab.de">
    <aucom:experiment factory="aucom.experiment.CastExperimentFactory" name="updater_equaldist_0-100_sample-2">
        <cast>
            <host>localhost</host>
            <sa name="fault">
                <wm lang="JAVA">cast.architecture.SubarchitectureWorkingMemory</wm>
                <tm lang="JAVA">cast.architecture.AlwaysPositiveTaskManager</tm>
                <mg lang="JAVA" name="skimmer" path="faultConnector.MemorySkimmer"/>
            </sa>
            <sa name="monitor">
                <wm lang="JAVA">cast.architecture.SubarchitectureWorkingMemory</wm>
                <tm lang="JAVA">cast.architecture.AlwaysPositiveTaskManager</tm>
                <mg lang="JAVA" name="monitor" path="monitor.Monitor"/>
            </sa>
            <sa name="updater">
                <wm lang="JAVA">cast.architecture.SubarchitectureWorkingMemory</wm>
                <tm lang="JAVA">cast.architecture.AlwaysPositiveTaskManager</tm>
                <mg lang="JAVA" name="updater" path="updater.Updater">
                    <args>--equal "0 100"</args>
                </mg>
            </sa>
        </cast>
        
        <aucom:input>
            <aucom:wd>~/dropbox/aucom</aucom:wd>
        </aucom:input>
        <aucom:output>~/dropbox/aucom</aucom:output>
    </aucom:experiment>
</aucom:experiments>
