<?xml version="1.0" encoding="UTF-8"?><aucom:experiments xmlns:aucom="http://www.cor-lab.de">
<aucom:experiment type="trainAndTest" name="testExperiment1">
        <aucom:input>
            <aucom:wd>./data/unittestdata/</aucom:wd>
            <aucom:train>norm100_2.obs</aucom:train>
             <aucom:train>norm100_3.obs</aucom:train>
             <aucom:test>prbfslam.obs</aucom:test>
        </aucom:input>
        <aucom:output>/home/rgolombe/workspace/aucom/data/</aucom:output>
        <aucom:model>
            <aucom:distribution type="aucom.models.t2gram.KDEProbabilityFactory">
                <aucom:parameter name="precision" type="java.lang.Double" value="1.0"/>
            </aucom:distribution>
            <aucom:Threshold type="aucom.models.classifier.StatisticalHysterisisAnomalyClassificator">
                <aucom:parameter name="mean" type="java.lang.Double" value="2.2"/>
                <aucom:parameter name="variance" type="java.lang.Double" value="0.1"/>
            </aucom:Threshold>
            <aucom:SlidingWindow size="5" overlap="2" >
            </aucom:SlidingWindow>
        </aucom:model>
    </aucom:experiment>
</aucom:experiments>
